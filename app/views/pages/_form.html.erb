<%= form_for @page do |f| %>
  <p>
    <%= f.label :parent_id %><br />
    <%= f.collection_select :parent_id, Page.all.order(:name), :id, :name, :include_blank => true %>
  </p>
  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p><%= f.submit "Submit", class: "btn btn-success" %></p>
<% end %>
